{
  "manifest_version": 2,

  "name": "nexogy-vdv-100",
  "description": "Nexogy's VDV-100 WebRTC Communicator",
  "version": "0.2",
  "background": {
    "scripts": [
      "js/jquery-1.11.2.min.js",
      "js/fcs.js", 
      "js/kandy.js", 
      "js/getcall.js"]
  },
  "icons": { "256": "assets/icon256.png",
             "32": "assets/icon32.png" },
  "browser_action": {
    "default_icon": "assets/nexogy-icon.png"
  },
  "permissions": [
    "notifications",
    "activeTab",
    "background",
    "tabs"
  ],
  "web_accessible_resources": [
    "permissions.html"
  ]
}